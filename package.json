{
  "name": "changelogd",
  "version": "0.1.4",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "cli": "jiti ./packages/cli/src/index.ts",
    "build": "unbuild .",
    "dev": "unbuild . --stub",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest"
  },
  "description": "Aggressively find a packages changelog (or releases) between versions.",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/changelogd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/changelogd.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "consola": "^2.15.3",
    "diff": "^5.0.0",
    "mlly": "^0.5.2",
    "mri": "^1.2.0",
    "ohash": "^0.1.5",
    "ohmyfetch": "^0.4.16",
    "pathe": "^0.3.8",
    "pkg-types": "^0.3.5",
    "semver": "^7.3.7",
    "unstorage": "^0.5.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@types/diff": "^5.0.2",
    "@types/node": "^18.7.21",
    "@types/semver": "^7.3.9",
    "bumpp": "^8.2.1",
    "changelogen": "^0.3.2",
    "eslint": "^8.24.0",
    "jiti": "^1.16.0",
    "pnpm": "^7.12.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  }
}
